<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:height="16dp"
                android:width="16dp"/>
            <solid
                android:color="#ffffff"/>
        </shape>
    </item>
    <item
        android:width="10dp"
        android:height="2dp"
        android:gravity="center"
        android:right="9dp"
        android:top="6dp"
        >
        <rotate
            android:fromDegrees="40">
            <shape android:shape="rectangle">
                <gradient
                    android:angle="-180"
                    android:endColor="#ff0066"
                    android:startColor="#d6006c"
                    />
            </shape>
        </rotate>
    </item>
    <item
        android:width="32dp"
        android:height="32dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item
        android:width="18dp"
        android:height="2dp"
        android:gravity="center"
        android:right="-8dp"
        android:top="-1dp"
        >
        <rotate android:fromDegrees="-50">
            <shape android:shape="rectangle">
                <gradient
                    android:angle="-90"
                    android:endColor="#ff0066"
                    android:startColor="#d6006c"
                    />
            </shape>
        </rotate>
    </item>
</layer-list>